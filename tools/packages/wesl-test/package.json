{
  "name": "wesl-test",
  "version": "0.6.18",
  "type": "module",
  "files": ["dist", "images", "src"],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepublishOnly": "run-s build",
    "test": "cross-env FORCE_COLOR=1 vitest",
    "test:once": "vitest run",
    "typecheck": "tsgo"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "main": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "dependencies": {
    "pngjs": "^7.0.0",
    "thimbleberry": "^0.2.10",
    "webgpu": "^0.3.8",
    "wesl": "workspace:^",
    "wesl-tooling": "workspace:x"
  },
  "devDependencies": {
    "@types/pngjs": "^6.0.0",
    "dependent_package": "workspace:x"
  },
  "peerDependencies": {
    "vitest": "^3.2.4",
    "vitest-image-snapshot": "workspace:x"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    },
    "vitest-image-snapshot": {
      "optional": true
    }
  },
  "keywords": [
    "shader",
    "snapshot",
    "test",
    "testing",
    "unit-test",
    "visual-regression",
    "WebGPU",
    "WESL",
    "WGSL"
  ]
}

{
  "name": "wesl-tooling",
  "version": "0.6.9",
  "type": "module",
  "files": ["dist"],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepublishOnly": "run-s build",
    "test": "cross-env FORCE_COLOR=1 vitest",
    "test:once": "vitest run",
    "typecheck": "tsgo"
  },
  "publishConfig": {
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "module": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "dependencies": {
    "glob": "^11.0.3",
    "import-meta-resolve": "^4.1.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "dependent_package": "workspace:x"
  },
  "peerDependencies": {
    "wesl": "workspace:^"
  }
}

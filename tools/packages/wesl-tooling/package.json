{
  "name": "wesl-tooling",
  "version": "0.6.21",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "repository": "github:wgsl-tooling-wg/wesl-js",
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "run-s build",
    "test": "cross-env FORCE_COLOR=1 vitest",
    "test:once": "vitest run",
    "typecheck": "tsgo"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "main": "dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "dependencies": {
    "glob": "^13.0.0",
    "import-meta-resolve": "^4.1.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "dependent_package": "workspace:x"
  },
  "peerDependencies": {
    "wesl": "workspace:^"
  }
}

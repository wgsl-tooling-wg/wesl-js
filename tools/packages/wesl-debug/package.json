{
  "name": "wesl-debug",
  "version": "0.6.15",
  "type": "module",
  "files": ["dist", "images", "src"],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepublishOnly": "run-s build",
    "test": "cross-env FORCE_COLOR=1 vitest",
    "test:once": "vitest run",
    "typecheck": "tsgo"
  },
  "publishConfig": {
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "module": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "dependencies": {
    "pngjs": "^7.0.0",
    "thimbleberry": "^0.2.10"
  },
  "devDependencies": {
    "@types/pngjs": "^6.0.0",
    "dependent_package": "workspace:x",
    "vitest-image-snapshot": "workspace:x",
    "webgpu": "^0.3.8"
  },
  "peerDependencies": {
    "wesl": "workspace:^"
  },
  "keywords": [
    "shader",
    "snapshot",
    "test",
    "testing",
    "unit-test",
    "visual-regression",
    "WebGPU",
    "WESL",
    "WGSL"
  ]
}

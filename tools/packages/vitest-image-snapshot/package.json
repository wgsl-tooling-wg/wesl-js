{
  "name": "vitest-image-snapshot",
  "version": "0.6.19",
  "type": "module",
  "files": ["dist", "src"],
  "repository": "github:wgsl-tooling-wg/wesl-js",
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "run-s build",
    "test": "cross-env FORCE_COLOR=1 vitest",
    "test:once": "vitest run",
    "typecheck": "tsgo"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./reporter": {
        "types": "./dist/reporter.d.ts",
        "import": "./dist/reporter.js"
      }
    },
    "main": "dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./reporter": {
      "types": "./src/reporter.ts",
      "import": "./src/reporter.ts"
    }
  },
  "dependencies": {
    "pixelmatch": "^6.0.0",
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.0"
  },
  "peerDependencies": {
    "vitest": "^3.2.4"
  },
  "license": "MIT",
  "keywords": [
    "image",
    "snapshot",
    "testing",
    "visual-regression",
    "vitest",
    "vitest-plugin"
  ]
}
